#include<iostream>
#include<limits>

int main(){
    //inteiro
    int inteiro = 436067945;
    short menores= 6745;
    long maior= 874563203;
    
    //caracteres
    char letra='m';
     
    //boleadores
    bool fouv= true;
    
    //flutuante
    float floatm= 3.76;
    double doubleg= 3.6428359031;
    
   std:: cout << "int: " <<inteiro<<std :: endl;
   std:: cout << "short: " <<menores<<std :: endl;
   std:: cout << "long: " <<maior<<std :: endl;
   std:: cout << "char: " <<letra<<std :: endl;
   std:: cout << "bool: " <<fouv<<std :: endl;
   std:: cout << "float: " <<floatm<<std :: endl;
   std:: cout << "double: " <<doubleg<<std :: endl;
 
    
    
    }
